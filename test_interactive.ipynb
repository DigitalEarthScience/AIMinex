    "import ipywidgets \n",
    "import numpy as np \n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "def plot_fct(w=1):\n",
    "    X = np.random.uniform(0, 5, size=100)\n",
    "    y = 2 * X + w * np.random.normal (size=100)\n",
    "    plt.scatter (X, y)\n",
    "ipywidgets. interact (plot_fct, W=(0, 5, 0.5))\n",
    "    "
